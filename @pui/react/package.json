{
  "name": "@pui/react",
  "version": "0.1.58",
  "module": "es/index.js",
  "main": "es/index.js",
  "typings": "es/index.d.ts",
  "files": [
    "es"
  ],
  "scripts": {
    "lint": "eslint --ignore-path=.eslintignore --ext .jsx,.ts,.tsx .",
    "lint-fix": "eslint --fix --ignore-path=.eslintignore --ext .js,.jsx,.ts,tsx .",
    "storybook": "start-storybook -p 6006 --docs",
    "build-storybook": "build-storybook --docs",
    "build": "git pull && rm -rf ./es && tsc -p tsconfig-build-es6.json && copyfiles -u 1 \"./src/**/*.scss\" \"./src/**/*.js\" \"./src/assets/**/*\" \"./src/styles/**/*\" \"./src/assets/**/*\" es && rm ./es/**/**/*.stories.scss",
    "vremove": "npm unpublish @pui/react@0.1.40 --registry https://devops.porsche-preview.cn/nexus/repository/npm-pcn-hosted/",
    "vrelease": "npm publish --registry https://devops.porsche-preview.cn/nexus/repository/npm-pcn-hosted/",
    "vrelease-old": "npm publish --registry http://s1.web.porsche-preview.cn/repository/npm-hosted/",
    "test": "jest",
    "vfixed": "standard-version",
    "audit": "npm audit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged & npm run test -- -o"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  },
  "formator": {
    "name": "Prettier",
    "class": "prettier_service.js",
    "filetypes": [
      "less",
      "sass",
      "scss",
      "ts"
    ]
  },
  "keywords": [],
  "author": "Porsche Digital China Web: to be the best web frontend team all arount the world",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@lottiefiles/react-lottie-player": "3.4.7",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-docs": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-interactions": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/builder-vite": "0.1.36",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.5.13",
    "@storybook/testing-library": "0.0.11",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/overlayscrollbars": "1.12.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.12.1",
    "@vitejs/plugin-react": "1.3.0",
    "babel-loader": "8.2.5",
    "copyfiles": "2.4.1",
    "css-loader": "5.2.7",
    "eslint": "8.17.0",
    "eslint-config-airbnb": "19.0.4",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass-loader": "10.1.1",
    "standard-version": "9.5.0",
    "style-loader": "2.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@pui/icons": "0.0.7",
    "async-validator": "3.5.2",
    "axios": "0.27.2",
    "classnames": "2.3.1",
    "i18next": "^22.1.4",
    "overlayscrollbars": "1.13.3",
    "overlayscrollbars-react": "0.3.0",
    "react-i18next": "^12.0.0",
    "react-transition-group": "4.4.2",
    "sass": "1.52.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.porsche-preview.cn:porsche-digital-china/web/pui.git"
  }
}
