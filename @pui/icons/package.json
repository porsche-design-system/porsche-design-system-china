    "build": "pui-tools build",
    "vinstall": "npm install --registry http://s1.web.porsche-preview.cn/repository/npm-group/",
    "vrelease": "npm publish --registry https://devops.porsche-preview.cn/nexus/repository/npm-pcn-hosted/",
